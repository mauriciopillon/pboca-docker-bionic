<Directory /var/www/boca/src>
       AllowOverride Options AuthConfig Limit
       Order Allow,Deny
       Allow from all
       AddDefaultCharset utf-8
</Directory>
<Directory /var/www/boca/src/private>
       AllowOverride Options AuthConfig Limit
       Deny from all
</Directory>
<Directory /var/www/boca>
       AllowOverride Options AuthConfig Limit
       Deny from all
</Directory>
Alias /boca /var/www/boca/src
